{
  "expo": {
    "name": "Native",
    "slug": "Native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/"
    }
  }
}
